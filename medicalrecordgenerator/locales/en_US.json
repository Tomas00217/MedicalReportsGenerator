{
  "diagnosis": {
    "variants": [
      {
        "condition": {
          "stroke_type": "ischemic"
        },
        "ischemicStroke": {
          "variants": [
            {
              "condition": {
                "AND": [
                  {"occlusion_position": true},
                  {"aspects_score": true}
                ]
              },
              "text": "Cerebral Infarction due to occlusion of $occlusion_position, ASPECT score $aspects_score. "
            },
            {
              "condition": {
                "AND": [
                  {"occlusion_position": true},
                  {"aspects_score": false}
                ]
              },
              "text": "Cerebral Infarction due to occlusion of $occlusion_position. "
            },
            {
              "condition": {
                "AND": [
                  {"occlusion_position": false},
                  {"aspects_score": false}
                ]
              },
              "text": "Cerebral Infarction without occlusion $imaging_type angiogram. "
            }
          ]
        }
      }
    ]
  },
  "onset": {
    "variants": [
      {
        "condition": {
          "AND": [
            {"onset_time": true},
            {"wake_up_stroke": false}
          ]
        },
        "text": "Onset on $onset_date at $onset_time. "
      },
      {
        "condition": {
          "AND": [
            {"onset_time": false},
            {"wake_up_stroke": true}
          ]
        },
        "text": "Onset on $onset_date, wake up stroke. "
      },
      {
        "condition": {
          "AND": [
            {"onset_time": false},
            {"wake_up_stroke": false}
          ]
        },
        "text": "Onset on $onset_date at an unknown time. "
        }
    ]
  },
  "admission": {
    "variants": [
      {
        "condition": {
          "AND": [
            {"admission_nihss": true},
            {"admission_type": true}
          ]
        },
        "text": "At baseline NIHSS $admission_nihss. Admitted to $admission_type. "
      },
      {
        "condition": {
          "AND": [
            {"admission_nihss": true},
            {"admission_type": false}
          ]
        },
        "text": "At baseline NIHSS $admission_nihss. "
      }
    ]
  },
  "treatment": {
    "variants": [
      {
        "condition": {
          "thrombolysis_done": true
        },
        "thrombolysis": {
          "variants": [
            {
              "condition": {
                "AND": [
                  {"ivt_dose": true},
                  {"ivt_treatment": true}
                ]
              },
              "text": "Intravenous thrombolysis with $ivt_dose mg of $ivt_treatment given $dtn minutes after arrival to hospital. "
            },
            {
              "condition": {
                "AND": [
                  {"ivt_dose": false},
                  {"ivt_treatment": true}
                ]
              },
              "text": "Intravenous thrombolysis with $ivt_treatment given $dtn minutes after arrival to hospital. "
            },
            {
              "condition": {
                "AND": [
                  {"ivt_dose": true},
                  {"ivt_treatment": false}
                ]
              },
              "text": "Intravenous thrombolysis at $ivt_dose mg given $dtn minutes after arrival to hospital. "
            },
            {
              "condition": {
                "AND": [
                  {"ivt_dose": false},
                  {"ivt_treatment": false}
                ]
              },
              "text": "Intravenous thrombolysis given $dtn minutes after arrival to hospital. "
            }
          ]
        }
      },
      {
        "condition": {
          "thrombolysis_done": false
        },
        "thrombolysis_not_provided": {
          "variants": [
            {
              "condition": {},
              "text": "Thrombolysis not given because of $thrombolysis_reasons. "
            }
          ]
        }
      },
      {
        "condition": {
          "thrombectomy_done": true
        },
        "thrombectomy": {
          "variants": [
            {
              "condition": {
                "tici_score": true
              },
              "text": "Mechanical thrombectomy performed $dtg minutes after arrival, TICI $tici_score $tici_score_meaning. "
            },
            {
              "condition": {
                "tici_score": false
              },
              "text": "Mechanical thrombectomy performed $dtg after arrival. "
            }
          ]
        }
      },
      {
        "condition": {
          "thrombectomy_done": true
        },
        "thrombectomy_not_provided": {
          "variants": [
            {
              "condition": {},
              "text": "Thrombectomy not given because of $thrombectomy_reasons. "
            }
          ]
        }
      },
      {
        "condition": {
          "thrombectomy_transport": true
        },
        "transport_for_thrombectomy": {
          "variants": [
            {
              "condition": {},
              "text": "Patient transported to interventional center for mechanical thrombectomy after $dio after arrival. "
            }
          ]
        }
      }
    ]
  },
  "follow_up_imaging": {
    "variants": [
      {
        "condition": {
          "findings": true,
          "aspects_score": true
        },
        "text": "$findings on follow up $imaging_type. ASPECT score $aspects_score. "
      },
      {
        "condition": {
          "findings": true,
          "aspects_score": false
        },
        "text": "$findings on follow up $imaging_type. "
      }
    ],
    "text": "No follow up imaging. "
  },
