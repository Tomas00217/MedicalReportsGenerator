    },
    "scores": {
      "text1": "NIHSS $nihss, mRS $mrs. ",
      "text2": "NIHSS $nihss. ",
      "text3": "mRS $mrs. "
    },
    "medication": {
      "text1": "Medication: $discharge_medication. "
    },
    "next_appointment": {
      "text1": "Next appointment scheduled to $contact_date. "
    }
  },
  "settings": {
    "date_time_format": "%b %d %Y at %H:%M",
    "date_format": "%b %d %Y",
